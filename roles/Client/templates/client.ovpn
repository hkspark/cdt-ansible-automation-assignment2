client
dev tun
proto udp
remote {{ openvpn_server_ip }} 1194
resolv-retry infinite
nobind
persist-key
persist-tun
tls-version-min 1.0
tls-version-max 1.0
cipher AES-256-CBC
auth SHA256
verb 3

<ca>
{{ lookup('file', 'ca.crt') }}
</ca>

<cert>
{{ lookup('file', openvpn_client_name + '.crt') }}
</cert>

<key>
{{ lookup('file', openvpn_client_name + '.key') }}
</key>
